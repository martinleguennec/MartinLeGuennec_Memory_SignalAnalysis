  author = {Yu, Qian and Herold, Fabian and Becker, Benjamin and {Klugah-Brown}, Benjamin and Zhang, Yanjie and Perrey, Stephane and Veronese, Nicola and Mueller, Notger and Kramer, Art and Zou, Liye},
