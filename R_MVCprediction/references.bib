  title = {Kinematic {{Variables Quantifying Upper-Extremity Performance After Stroke During Reaching}} and {{Drinking From}} a {{Glass}}},
  author = {Murphy, Margit Alt and Will{\'e}n, Carin and Sunnerhagen, Katharina S.},
